{
    "esm": {
        "version": "2.5",
        "website": "http://www.ezservermonitor.com",
        "check_updates": false,
        "auto_refresh": 0,
        "theme": "blue",
        "custom_title": ""
    },
    "cpu": {
        "enable_temperature": false
    },
    "disk": {
        "show_tmpfs": false,
        "show_loop": false,
        "show_filesystem": true,
        "ignore_mounts": []
    },
    "ping": {
        "hosts": []
    },
    "last_login": {
        "enable": false,
        "max": 5
    },
    "services": {
        "show_port": true,
        "list": [
            {
                "name": "VS Code",
                "host": "localhost",
                "port": 8080,
                "protocol": "tcp"
            },
            {
                "name": "Label Studio",
                "host": "localhost",
                "port": 6006,
                "protocol": "tcp"
            },
            {
                "name": "TensorBoard",
                "host": "localhost",
                "port": 6007,
                "protocol": "tcp"
            },
            {
                "name": "FiftyOne",
                "host": "localhost",
                "port": 6008,
                "protocol": "tcp"
            },
            {
                "name": "SSH",
                "host": "localhost",
                "port": 22,
                "protocol": "tcp"
            }
        ]
    }
}
